    private void jButton_devActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        num1 = Double.parseDouble(jTextField1.getText());
        operator = "/";
        jTextField1.setText("");
    }                                           

    private void jButton_equalActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // TODO add your handling code here:
        num2 = Double.parseDouble(jTextField1.getText());
        switch (operator) {
            case "+":
                result = num1 + num2;
                break; 
            case "-":
                result = num1 - num2;
                break;
            case "*":
                result = num1 * num2;
                break;
            case "/":
                if (num2 == 0) {
                    jTextField1.setText("0으로 나눌 수 없습니다.");
                return; 
                } else {
                    result = num1 / num2;
        }
        break;
        }   
        jTextField1.setText(String.valueOf(result));
    }                                             

    private void jButton_PMActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        double num = Double.parseDouble(jTextField1.getText());
        num = num * -1;
        jTextField1.setText(String.valueOf(num));
    }                                          

    private void jButton_1xActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        double num = Double.parseDouble(jTextField1.getText());
        
        if (num == 0) {
            jTextField1.setText("0으로 나눌 수 없습니다.");
        } else {
            num = 1 / num;
            jTextField1.setText(String.valueOf(num));
        }
    }                                          

    private void jButton_npActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        int num = (int)Double.parseDouble(jTextField1.getText());
        int result = 1; 
        for (int i = 1; i <= num; i++) {
            result = result * i;
        }
        jTextField1.setText(String.valueOf(result));
    }                                          

    private void jButton_x3ActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        double num = Double.parseDouble(jTextField1.getText());
        double result = Math.pow(num, 3);
        jTextField1.setText(String.valueOf(result));
    }                                          

    private void jButton_logActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
