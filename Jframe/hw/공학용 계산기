    private void jButton_BackspaceActionPerformed(java.awt.event.ActionEvent evt) {                                                  
        // TODO add your handling code here:
        String cntText = jTextField1.getText();
        int len = cntText.length();
        String newText = cntText.substring(0, len - 1);
        jTextField1.setText(newText);
    }                                                 

    private void jButton_PlusActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
        num1 = Double.parseDouble(jTextField1.getText());
        operator = "+";
        jTextField1.setText("");
    }                                            

    private void jButton_minusActionPerformed(java.awt.event.ActionEvent evt) {                                              
